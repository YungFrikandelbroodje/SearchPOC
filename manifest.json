{
  "name": "POC",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "AAA",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": ["src/content/jquery-3.5.1.min.js", "src/bg/functions.js", "src/bg/background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "POC",
    "default_popup": "src/popup/popup.html"
  },

  "permissions": ["cookies", "storage", "unlimitedStorage", "webRequest", "webRequestBlocking", "http://*/*", "https://*/*"],
  "content_scripts": [
    {
      "all_frames": false,
      "js": ["src/content/jquery-3.5.1.min.js", "src/content/content.js"],
      "match_about_blank": false,
      "matches": ["<all_urls>"]
    }
  ]
}
